{
  "manifest_version": 2,
  "name": "Bypass Zajuna SENA",
  "version": "1.1",
  "description": "Permite el acceso a Zajuna debido al error de bloqueo.",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://zajuna.sena.edu.co/*"
  ],
  "icons": {
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "64": "icons/icon-64.png",
    "96": "icons/icon-96.png",
    "128": "icons/icon-128.png",
    "256": "icons/icon-256.png"
  },
  "background": {
    "scripts": ["background.js"]
  }
}
